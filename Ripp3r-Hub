local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Key", HidePremium = false, SaveConfig = true, IntroText = "Ripp3r-Hub Key"})

_G.Key = "Psnemmsns"
_G.KeyInput = "string"

function MakeScriptHub()

end

function CorrectKeyNotification
OrionLib:MakeNotification({
	Name = "Correct-Key!!",
	Content = "You have entered the correct key??",
	Time = 5
})
end

local Tab = Window:MakeTab({
	Name = "Enter Key",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Key",
	Default = "Enter Key",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = "Value"
	end	  
})

Tab:AddButton({
	Name = "Check-Key",
	Callback = function()
      		if _G.KeyInput == _G.Key then
		MakeScriptHub()
		CorrectKeyNotification()
		end
  	end    
})
